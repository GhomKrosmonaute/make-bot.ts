{
  "name": "@ghom/bot.ts-cli",
  "version": "8.3.1",
  "description": "CLI for make Discord bots in TypeScript quickly with discord.js v14.",
  "main": "bin/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src events.json update-readme.js",
    "test": "npm run build && npm pack --pack-destination ./pack",
    "prepublishOnly": "npm run format && tsc"
  },
  "prettier": {
    "semi": false
  },
  "keywords": [
    "bot.ts",
    "discord",
    "bot",
    "typescript",
    "cli"
  ],
  "author": "ghom",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bot-ts/cli.git"
  },
  "bugs": {
    "url": "https://github.com/bot-ts/cli/issues"
  },
  "homepage": "https://ghom.gitbook.io/bot-ts/",
  "bin": {
    "bot": "./bin/index.js"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/node": "^22.0.2",
    "@types/string-similarity": "^4.0.0",
    "prettier": "^2.6.2",
    "types-package-json": "^2.0.39",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.1",
    "boxen": "=5.1.2",
    "commander": "^12.1.0",
    "discord.js": "^14.15.2",
    "figlet": "^1.5.2",
    "loading-cli": "^1.1.0",
    "string-similarity": "^4.0.4"
  },
  "engines": {
    "node": ">=22.x.x",
    "npm": ">=9.x.x"
  }
}
