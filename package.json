{
  "name": "@ghom/bot.ts-cli",
  "version": "9.0.0",
  "description": "CLI for make Discord bots in TypeScript quickly with discord.js v14.",
  "main": "bin/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src events.json update-readme.js",
    "test": "npm run build && npm pack --pack-destination ./pack",
    "prepublishOnly": "npm run format && tsc"
  },
  "prettier": {
    "semi": false
  },
  "keywords": [
    "bot.ts",
    "discord",
    "bot",
    "typescript",
    "cli"
  ],
  "author": "ghom",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bot-ts/cli.git"
  },
  "bugs": {
    "url": "https://github.com/bot-ts/cli/issues"
  },
  "homepage": "https://ghom.gitbook.io/bot-ts/",
  "bin": {
    "bot": "./bin/index.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.1",
    "boxen": "=5.1.2",
    "commander": "^12.1.0",
    "discord-api-types": "^0.37.105",
    "ejs": "^3.1.10",
    "figlet": "1.8.0",
    "inquirer": "^12.1.0",
    "loading-cli": "^1.1.0",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/figlet": "1.7.0",
    "@types/node": "^22.0.2",
    "@types/prettier": "^2.7.3",
    "types-package-json": "^2.0.39",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=22.x.x"
  }
}
